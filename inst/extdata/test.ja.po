"#| fig.cap: Sun arise each and every morning\n"
"pie(\n"
"  c(Sky = 78, \"Sunny side of pyramid\" = 17, \"Shady side of pyramid\" = 5), \n"
"  init.angle = 315, \n"
"  col = c(\"deepskyblue\", \"yellow\", \"yellow3\"), \n"
"  border = FALSE\n"
")\n"
